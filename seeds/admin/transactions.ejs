<!DOCTYPE html>
<html lang="en">
<!-- xhtml/index.html08:17:22 -->

<head>
    <meta charset="utf-8" />
    <meta http-equiv="X-UA-Compatible" content="IE=edge" />
    <meta name="viewport" content="width=device-width,initial-scale=1" />
    <title>Transactions | NFT Talent</title>

    <!-- Favicon icon -->
    <link rel="icon" type="image/x-icon" sizes="16x16" href="/assets/img/favicon/favicon.ico" />

    <link rel="stylesheet" href="/assets/vendor/datatables/css/jquery.dataTables.min.css" />
    <link rel="stylesheet" href="/assets/vendor/datatables/css/responsive.dataTables.min.css" />
    <link rel="stylesheet" href="/admin/assets/vendor/bootstrap-select/dist/css/bootstrap-select.min.css" />

    <!-- <link href="css/style.css" rel="stylesheet"> -->
    <link href="/admin/css/admin-style.css" rel="stylesheet" />
</head>

<body>
    <!--*******************
            Preloader start
        ********************-->
    <div id="preloader">
        <div class="sk-three-bounce">
            <div class="sk-child sk-bounce1"></div>
            <div class="sk-child sk-bounce2"></div>
            <div class="sk-child sk-bounce3"></div>
        </div>
    </div>
    <!--*******************
            Preloader end
        ********************-->

    <!--**********************************
            Main wrapper start
        ***********************************-->
    <div id="main-wrapper">
        <!-- Include Header -->
        <%- include('header'); -%>

            <!-- Include Sidebar -->
            <%- include('sidebar'); -%>

                <!--**********************************
                Content body start
            ***********************************-->
                <div class="content-body">
                    <!-- row -->
                    <div class="container-fluid">
                        <div class="row">
                            <div class="col-12">
                                <div class="card">
                                    <div class="card-header">
                                        <h4 class="card-title">Transactions</h4>
                                    </div>
                                    <div class="card-body">
                                        <div class="table-responsive">
                                            <table id="transactionTable" class="display">
                                                <thead>
                                                    <tr>
                                                        <th>Transaction Hash</th>
                                                        <th>Transaction Type</th>
                                                        <th>User</th>
                                                        <th>Date&Time</th>
                                                        <th>Amount</th>
                                                        <th>NFT Art</th>
                                                    </tr>
                                                </thead>
                                                <tbody></tbody>
                                            </table>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
                <!--**********************************
                Content body end
            ***********************************-->
                <!-- </div> -->
                <!-- </div> -->
                <!-- </div> -->

                <!--**********************************
                Footer start
            ***********************************-->

                <%- include('footer'); -%>

                    <!--**********************************
                Footer end
            ***********************************-->
    </div>

    <div class="nft-details-modal-otr">
        <a href="" class="close-icn">
            <img src="/assets/img/cancel.svg" alt="close">
        </a>
        <div class="nft-details-modal">
            <div class="row">
                <div class="col-md-6">
                    <div class="nft-image-show">
                        <img id="nftImage" src="" alt="NFT">
                    </div>
                </div>
                <div class="col-md-6 d-flex flex-column justify-content-center">
                    <div class="row">
                        <div class="col-6"><label>Name:</label></div>
                        <div class="col-6"><label id="nftName"></label></div>
                    </div>
                    <div class="row">
                        <div class="col-6"><label>Price:</label></div>
                        <div class="col-6"><label id="nftPrice"></label></div>
                    </div>
                    <div class="row">
                        <div class="col-6"><label>bStatus:</label></div>
                        <div class="col-6"><label id="nftStatus"></label></div>
                    </div>
                    <div class="row">
                        <div class="col-6"><label>Sale Type:</label></div>
                        <div class="col-6"><label id="nftSaleType"></label></div>
                    </div>
                </div>
            </div>
        </div>
    </div>
    <!--**********************************
            Main wrapper end
        ***********************************-->

    <!--**********************************
            Scripts
        ***********************************-->

    <!-- Required vendors -->

    <script src="/admin/assets/vendor/bootstrap-select/dist/js/bootstrap-select.min.js"></script>
    <script src="/admin/js/custom.min.js"></script>
    <script src="/admin/js/deznav-init.js"></script>
    <script src="/assets/vendor/datatables/js/jquery.dataTables.min.js"></script>
    <script src="/assets/vendor/datatables/js/dataTables.responsive.min.js"></script>
    <!-- <script src="https://cdnjs.cloudflare.com/ajax/libs/moment.js/2.29.4/moment.min.js"></script> -->


    <script src="/admin/js/transactions.js"></script>

</body>

</html>