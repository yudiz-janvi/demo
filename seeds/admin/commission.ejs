<!DOCTYPE html>
<html lang="en">

<head>
    <meta charset="utf-8" />
    <meta http-equiv="X-UA-Compatible" content="IE=edge" />
    <meta name="viewport" content="width=device-width,initial-scale=1" />
    <title>Manage Commission | NFT Talent</title>
    <!-- Favicon icon -->
    <link rel="icon" type="image/x-icon" sizes="16x16" href="/assets/img/favicon/favicon.ico" />
    <!-- <link
            href="/assets/vendor/bootstrap-select/dist/css/bootstrap-select.min.css"
            rel="stylesheet"
        />

        <link href="/admin/css/admin-style.css" rel="stylesheet" /> -->
    <link rel="icon" type="image/x-icon" sizes="16x16" href="/assets/img/favicon/favicon.ico" />
    <link rel="stylesheet" href="/admin/assets/vendor/bootstrap-select/dist/css/bootstrap-select.min.css" />

    <!-- <link href="css/style.css" rel="stylesheet"> -->
    <link href="/admin/css/admin-style.css" rel="stylesheet" />
</head>

<body>
    <!--*******************
        Preloader start
    ********************-->
    <div id="preloader">
        <div class="sk-three-bounce">
            <div class="sk-child sk-bounce1"></div>
            <div class="sk-child sk-bounce2"></div>
            <div class="sk-child sk-bounce3"></div>
        </div>
    </div>
    <!--*******************
        Preloader end
    ********************-->

    <!--**********************************
        Main wrapper start
    ***********************************-->
    <div id="main-wrapper" class="show">
        <!--**********************************
            Nav header start
        ***********************************-->

        <!--**********************************
            Nav header end
        ***********************************-->

        <!--**********************************
            Header start
        ***********************************-->
        <%- include('header'); -%>
            <!--**********************************
            Header end ti-comment-alt
        ***********************************-->

            <!--**********************************
            Sidebar start
        ***********************************-->
            <%- include('sidebar'); -%>
                <!--**********************************
            Sidebar end
        ***********************************-->

                <!--**********************************
            Content body start
        ***********************************-->
                <div class="content-body">
                    <!-- row -->
                    <div class="container-fluid">
                        <div class="row">

                            <div class="col-xl-6 col-xxl-12">
                                <div class="row">
                                    <div class="col-xl-12 col-lg-12">
                                        <div class="card">
                                            <div
                                                class="card-header border-0 d-xl-flex d-lg-block d-md-flex d-sm-flex d-block">
                                                <div class="mr-2">
                                                    <h4 class="fs-20 text-black">
                                                        Update PlatForm Fee
                                                    </h4>
                                                </div>
                                                <div class="ml-2">
                                                    <div class="select-main all-chain">
                                                        <div class="select"><select id="selectWithChain"
                                                                class="select-hidden">
                                                                <option value="97">Binance Smart Chain -
                                                                    Testnet
                                                                </option>
                                                                <option value="5">Goerli - Testnet</option>
                                                                <option value="43113">Avalanche fuji -
                                                                    Testnet</option>
                                                                <option value="80001">Mumbai - Testnet
                                                                </option>
                                                            </select>
                                                            <!-- <div class="select-styled">Avalanche fuji - Testnet</div> -->
                                                            <!-- <ul class="select-options" style="display: none;">
                                                                <li rel="" id="allChain">
                                                                    All chain
                                                                </li>
                                                                <li rel="97" id="chain97">Binance Smart Chain - Testnet
                                                                </li>
                                                                <li rel="5" id="chain5">Goerli - Testnet</li>
                                                                <li rel="43113" id="chain43113">Avalanche fuji - Testnet
                                                                </li>
                                                                <li rel="80001" id="chain80001">Mumbai - Testnet</li>
                                                            </ul> -->
                                                        </div>
                                                    </div>
                                                </div>
                                            </div>
                                            <div class="card-body tab-content card-table">
                                                <div class="form-validation">
                                                    <span class="form-valide">
                                                        <div class="row">
                                                            <div class="col-xl-12">
                                                                <div class="form-group row">
                                                                    <label class="col-lg-5 col-form-label"
                                                                        for="edit_commission"><span>New
                                                                            PlatForm Fee</span>
                                                                        <span class="text-danger">*</span>
                                                                    </label>
                                                                    <div class="input-group col-lg-4">
                                                                        <input type="number" class="form-control"
                                                                            id="txtCommission" name="edit_commission"
                                                                            placeholder="PlatForm Fee" max="100" />
                                                                        <span class="input-group-text">%</span>

                                                                        <div id="commissionPercentage-error"
                                                                            class="invalid-feedback animated fadeInUp">
                                                                        </div>
                                                                    </div>
                                                                </div>
                                                                <div class="form-group row">
                                                                    <label class="col-lg-5 col-form-label"
                                                                        for="edit_commission"><span>New
                                                                            Referrer Percentage</span>
                                                                        <span class="text-danger">*</span>
                                                                    </label>
                                                                    <div class="input-group col-lg-4">
                                                                        <input type="number" class="form-control"
                                                                            id="edit_referPercent"
                                                                            name="edit_commission"
                                                                            placeholder="Refer Percentage" max="100" />
                                                                        <span class="input-group-text">%</span>
                                                                        <div id="commissionPercentage-error"
                                                                            class="invalid-feedback animated fadeInUp">
                                                                        </div>
                                                                    </div>
                                                                </div>
                                                                <div class="form-group row">
                                                                    <label class="col-lg-5 col-form-label"
                                                                        for="edit_commission"><span>New
                                                                            Referee Percentage</span>
                                                                        <span class="text-danger">*</span>
                                                                    </label>
                                                                    <div class="input-group col-lg-4">
                                                                        <input type="number" class="form-control"
                                                                            id="edit_referrelPercent"
                                                                            name="edit_commission"
                                                                            placeholder="Referrel Percentage"
                                                                            max="100" />
                                                                        <span class="input-group-text">%</span>
                                                                        <div id="commissionPercentage-error"
                                                                            class="invalid-feedback animated fadeInUp">
                                                                        </div>
                                                                    </div>
                                                                </div>

                                                                <center>
                                                                    <label for="decimalError" id="decimalError"
                                                                        class="text-danger d-none"></label>
                                                                </center>
                                                                <div class="form-group row">
                                                                    <div class="col-lg-8 ml-auto">
                                                                        <button type="button" id="btnUpdatePercentage"
                                                                            class="btn btn-primary">
                                                                            Update
                                                                        </button>
                                                                    </div>
                                                                </div>
                                                            </div>
                                                        </div>
                                                    </span>
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                            </div>

                        </div>
                    </div>

                </div>
                <!--**********************************
            Content body end
        ***********************************-->

                <!--**********************************
            Footer start
        ***********************************-->

                <%- include('footer'); -%>

                    <!--**********************************
            Footer end
        ***********************************-->
    </div>
    <!--**********************************
        Main wrapper end
    ***********************************-->

    <!--**********************************
        Scripts
    ***********************************-->
    <script src="/assets/js/web3.min.js"></script>
    <!-- <script src="/assets/js/contracts.web3.js"></script> -->
    <script src="/admin/assets/vendor/bootstrap-select/dist/js/bootstrap-select.min.js"></script>
    <script src="/admin/js/custom.min.js"></script>
    <script src="/admin/js/deznav-init.js"></script>

    <!-- <script src="../assets/vendor/bootstrap-select/dist/js/bootstrap-select.min.js"></script>
        <script src="../assets/js/custom.min.js"></script>
        <script src="../assets/js/deznav-init.js"></script>

        <script src="../assets/vendor/web3/web3.min.js"></script> -->


    <script src="../admin/js/commission.js"></script>
</body>

</html>